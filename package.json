{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "A frontend project template for Vue",
  "author": "tiexin.gong <tiexin.gong@qq.com>",
  "private": true,
  "homepage": "https://github.com/gongtiexin/vue-template#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gongtiexin/vue-template.git"
  },
  "bugs": {
    "url": "https://github.com/gongtiexin/vue-template.git/issues"
  },
  "engines": {
    "npm": ">=6.11.3",
    "node": ">=12.10.0"
  },
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.js",
    "build": "webpack --progress --config build/webpack.prod.js",
    "prettier": "prettier -c --write \"{**/*,*}.{js,vue,less,sass,json,md}\""
  },
  "lint-staged": {
    "*": [
      "yarn prettier"
